{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "framework": "other",
  "outputDirectory": "."
}
{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "framework": "static",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
